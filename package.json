{
  "name": "biblebench",
  "version": "1.0.0",
  "description": "LLM benchmark for Christian scripture accuracy and theological understanding",
  "type": "module",
  "scripts": {
    "eval:dev": "evalite watch",
    "eval": "evalite run",
    "eval:ui": "evalite serve",
    "eval:export": "evalite export --output=docs/traces --basePath=/traces",
    "eval:merge": "node scripts/merge-eval-results.js",
    "dashboard:build": "node scripts/build-dashboard.js",
    "dashboard:serve": "pnpm dlx serve ./docs",
    "dashboard:dev": "pnpm dashboard:build && pnpm dashboard:serve",
    "deploy": "pnpm eval:export && pnpm eval:merge && pnpm dashboard:build"
  },
  "keywords": [
    "llm",
    "benchmark",
    "theology",
    "bible",
    "scripture",
    "evaluation"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.26.2",
  "devDependencies": {
    "@ai-sdk/provider": "2.0.0",
    "@types/node": "^25.0.3",
    "evalite": "1.0.0-beta.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "ai": "^5.0.116",
    "autoevals": "^0.0.131",
    "zod": "^4.2.1"
  },
  "pnpm": {
    "overrides": {
      "zod": "^4.2.1"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
