{
  "name": "biblebench",
  "version": "1.0.0",
  "description": "Industry-standard LLM benchmark for Christian scripture accuracy and theological understanding",
  "type": "module",
  "scripts": {
    "eval:dev": "evalite watch",
    "eval": "evalite run",
    "eval:ui": "evalite ui",
    "eval:scripture": "evalite run evals/scripture/",
    "eval:theology": "evalite run evals/theology/",
    "eval:quick": "evalite run evals/scripture/verse-recall.eval.ts && evalite run evals/theology/core-doctrines.eval.ts"
  },
  "keywords": [
    "llm",
    "benchmark",
    "theology",
    "bible",
    "scripture",
    "evaluation"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.26.2",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "evalite": "1.0.0-beta.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@ai-sdk/openai": "^3.0.1",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "ai": "^5.0.116",
    "autoevals": "^0.0.131",
    "zod": "^4.2.1"
  },
  "pnpm": {
    "overrides": {
      "@ai-sdk/provider": "3.0.0",
      "zod": "^4.2.1"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
